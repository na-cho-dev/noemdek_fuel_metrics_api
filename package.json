{
  "name": "express-template",
  "version": "1.0.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "pnpm install && tsc",
    "prebuild": "pnpm run clean",
    "clean": "rm -rf dist",
    "build:watch": "tsc --watch",
    "start": "node dist/app.js",
    "start:prod": "NODE_ENV=production node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "dev:debug": "ts-node-dev --inspect --respawn --transpile-only src/app.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:unit": "jest --testPathPattern='src/tests/.*\\.test\\.ts$'",
    "test:integration": "jest --testPathPattern='src/tests/.*routes.*\\.test\\.ts$'",
    "validate": "pnpm run lint && pnpm run format:check && pnpm run test",
    "seed:db": "ts-node scripts/import-fuel-data.ts",
    "reset:db": "ts-node scripts/reset-database.ts"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@jest/globals": "^30.0.4",
    "@types/bcryptjs": "^3.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.2",
    "@types/jest": "^30.0.0",
    "@types/json2csv": "^5.0.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.10",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^30.0.4",
    "mongodb-memory-server": "^10.1.4",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@types/helmet": "^4.0.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "json2csv": "6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "redis": "^5.5.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5",
    "zod": "^3.25.74"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "description": "",
  "author": ""
}
